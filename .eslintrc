{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": ["eslint-config-floobits", "plugin:react/recommended"],
  "plugins": [
    "react"
  ],
  "rules": {
    "react/no-deprecated": [1],
    "react/no-is-mounted": [1],
    "strict": [0],
  },
  "settings": {
    "ecmascript": 6,
    "jsx": true
  }
}
